    <%- include('./partials/header.ejs', {title: show.showName}) %>
    <%- include('./partials/navbar.ejs', {username: locals.username}) %>

    <div class="container">
        <div class="row">
        <h2><%= show.showName %></h2>
        <p>popularity: <%= show.popularity %></p>
        <p>first air date: <%= show.firstAirDate.formatted() %></p>
        <p>last episode aired on <%= show.lastEpisodeToAir.airDate.formatted() %></p>
        <p>favorite: <%= show.userShow?.favorite ?? false %></p>
        <p>watched: <%= show.userShow?.hasWatched ?? false %></p>
        <img class="movie-poster" src="<%= fullPosterPath %>" alt="<%= show.showName %>">
        </div>
    </div>

    <%- include('./partials/footer.ejs') %>
